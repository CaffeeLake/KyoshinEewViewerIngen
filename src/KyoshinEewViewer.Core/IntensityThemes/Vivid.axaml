<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Styles.Resources>
    <Color x:Key="UnknownForeground">Black</Color>
    <Color x:Key="UnknownBackground">#808080</Color>

    <Color x:Key="ErrorForeground">#b30f20</Color>
    <Color x:Key="ErrorBackground">#ffff6c</Color>

    <Color x:Key="Int0Foreground">White</Color>
    <Color x:Key="Int0Background">#808080</Color>
    <Color x:Key="Int1Foreground">DarkGray</Color>
    <Color x:Key="Int1Background">Snow</Color>
    <Color x:Key="Int2Foreground">White</Color>
    <Color x:Key="Int2Background">DodgerBlue</Color>
    <Color x:Key="Int3Foreground">White</Color>
    <Color x:Key="Int3Background">MediumSeaGreen</Color>
    <Color x:Key="Int4Foreground">White</Color>
    <Color x:Key="Int4Background">Goldenrod</Color>

    <Color x:Key="Int5LowerForeground">White</Color>
    <Color x:Key="Int5LowerBackground">DarkOrange</Color>
    <Color x:Key="Int5UpperForeground">White</Color>
    <Color x:Key="Int5UpperBackground">DarkOrange</Color>

    <Color x:Key="Int6LowerForeground">White</Color>
    <Color x:Key="Int6LowerBackground">Crimson</Color>
    <Color x:Key="Int6UpperForeground">White</Color>
    <Color x:Key="Int6UpperBackground">Crimson</Color>

    <Color x:Key="Int7Foreground">White</Color>
    <Color x:Key="Int7Background">DeepPink</Color>
  </Styles.Resources>
</Styles>
