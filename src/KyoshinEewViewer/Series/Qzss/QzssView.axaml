                            </ItemsControl.ItemTemplate>
                        </ListBox>
                </ScrollViewer>
            </Border>
        </Grid>

        <StackPanel Grid.Column="2" Spacing="5" VerticalAlignment="Bottom">
            <Border CornerRadius="4" Padding="0,-1,0,0" ClipToBounds="True" IsVisible="False">
                <StackPanel>
                    <Border Grid.Row="0" Background="{DynamicResource DockTitleBackgroundColor}" CornerRadius="4,4,0,0">
                        <TextBlock Text="衛星の位置" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Foreground="{DynamicResource SubForegroundColor}" />
                    </Border>
                    <Border Grid.Row="1" Background="{DynamicResource DockBackgroundColor}" CornerRadius="0,0,4,4" ClipToBounds="True">
                        <Panel Height="250" />
                    </Border>
                </StackPanel>
            </Border>
        </StackPanel>
    </Grid>
</UserControl>
