<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
             xmlns:AvalonEdit="using:AvaloniaEdit"
             xmlns:actions="clr-namespace:KyoshinEewViewer.Services.Workflows.BuiltinActions;assembly=KyoshinEewViewer"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="actions:WebhookAction" x:CompileBindings="True"
             x:Class="KyoshinEewViewer.Services.Workflows.BuiltinActions.WebhookActionControl">
    <StackPanel>
        <TextBlock Text="URL" />
        <TextBox Text="{Binding Url}" HorizontalAlignment="Stretch" />
        <TextBlock Text="最新のレスポンス" />
        <TextBlock Text="{Binding LatestResponse}" Margin="5,0" />
    </StackPanel>
</UserControl>
