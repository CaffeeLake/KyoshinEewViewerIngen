<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:core="using:KyoshinEewViewer.Core"
			 xmlns:sty="using:FluentAvalonia.Styling"
             x:Class="KyoshinEewViewer.Desktop.App"
             Name="KyoshinEewViewer for ingen">
	<Application.Styles>
		<StyleInclude Source="avares://KyoshinEewViewer.Core/Common.axaml" />
		<core:KyoshinEewViewerWindowTheme />
        <core:KyoshinEewViewerIntensityTheme />
		<sty:FluentAvaloniaTheme UseSystemFontOnWindows="False" PreferUserAccentColor="True" />
        <StyleInclude Source="avares://AvaloniaEdit/Themes/Fluent/AvaloniaEdit.xaml" />
	</Application.Styles>

	<NativeMenu.Menu>
		<NativeMenu>
			<NativeMenuItem Header="設定" Click="OpenSettingsClicked" />
		</NativeMenu>
	</NativeMenu.Menu>
</Application>
